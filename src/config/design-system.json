{
  "name": "Arms Complex Design System",
  "version": "1.0.0",
  "description": "Complete design tokens for tactical/firearms e-commerce platform",
  
  "fonts": {
    "primary": {
      "family": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
      "description": "Default system font stack"
    },
    "weights": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "sizes": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem"
    }
  },

  "colors": {
    "background": {
      "hsl": "0 0% 7%",
      "hex": "#121212",
      "rgb": "rgb(18, 18, 18)",
      "usage": "Main page background, body background"
    },
    "foreground": {
      "hsl": "0 0% 95%",
      "hex": "#F2F2F2",
      "rgb": "rgb(242, 242, 242)",
      "usage": "Primary text color on dark backgrounds"
    },
    "card": {
      "hsl": "0 0% 10%",
      "hex": "#1A1A1A",
      "rgb": "rgb(26, 26, 26)",
      "usage": "Card backgrounds, elevated surfaces"
    },
    "cardForeground": {
      "hsl": "0 0% 95%",
      "hex": "#F2F2F2",
      "rgb": "rgb(242, 242, 242)",
      "usage": "Text on card surfaces"
    },
    "popover": {
      "hsl": "0 0% 10%",
      "hex": "#1A1A1A",
      "rgb": "rgb(26, 26, 26)",
      "usage": "Dropdown menus, popovers, modals"
    },
    "popoverForeground": {
      "hsl": "0 0% 95%",
      "hex": "#F2F2F2",
      "rgb": "rgb(242, 242, 242)",
      "usage": "Text in popovers and dropdowns"
    },
    "primary": {
      "hsl": "84 28% 35%",
      "hex": "#5C7244",
      "rgb": "rgb(92, 114, 68)",
      "usage": "Primary buttons, main CTAs, links, brand color (olive/tactical green)"
    },
    "primaryForeground": {
      "hsl": "0 0% 98%",
      "hex": "#FAFAFA",
      "rgb": "rgb(250, 250, 250)",
      "usage": "Text on primary buttons"
    },
    "secondary": {
      "hsl": "0 0% 15%",
      "hex": "#262626",
      "rgb": "rgb(38, 38, 38)",
      "usage": "Secondary buttons, alternative surfaces"
    },
    "secondaryForeground": {
      "hsl": "0 0% 95%",
      "hex": "#F2F2F2",
      "rgb": "rgb(242, 242, 242)",
      "usage": "Text on secondary buttons"
    },
    "muted": {
      "hsl": "0 0% 15%",
      "hex": "#262626",
      "rgb": "rgb(38, 38, 38)",
      "usage": "Muted backgrounds, disabled states background"
    },
    "mutedForeground": {
      "hsl": "0 0% 60%",
      "hex": "#999999",
      "rgb": "rgb(153, 153, 153)",
      "usage": "Subtle text, placeholders, secondary info, metadata"
    },
    "accent": {
      "hsl": "45 93% 47%",
      "hex": "#E8A70A",
      "rgb": "rgb(232, 167, 10)",
      "usage": "Accent highlights, badges, special callouts (golden amber)"
    },
    "accentForeground": {
      "hsl": "0 0% 10%",
      "hex": "#1A1A1A",
      "rgb": "rgb(26, 26, 26)",
      "usage": "Text on accent backgrounds"
    },
    "destructive": {
      "hsl": "0 84% 60%",
      "hex": "#EB4D4D",
      "rgb": "rgb(235, 77, 77)",
      "usage": "Error states, delete buttons, danger alerts"
    },
    "destructiveForeground": {
      "hsl": "0 0% 98%",
      "hex": "#FAFAFA",
      "rgb": "rgb(250, 250, 250)",
      "usage": "Text on destructive buttons"
    },
    "border": {
      "hsl": "0 0% 20%",
      "hex": "#333333",
      "rgb": "rgb(51, 51, 51)",
      "usage": "All borders, dividers, separators"
    },
    "input": {
      "hsl": "0 0% 20%",
      "hex": "#333333",
      "rgb": "rgb(51, 51, 51)",
      "usage": "Input field borders, form elements"
    },
    "ring": {
      "hsl": "84 28% 35%",
      "hex": "#5C7244",
      "rgb": "rgb(92, 114, 68)",
      "usage": "Focus ring color for accessibility"
    },
    "tactical": {
      "hsl": "84 28% 35%",
      "hex": "#5C7244",
      "rgb": "rgb(92, 114, 68)",
      "usage": "Military/tactical themed elements (same as primary)"
    },
    "tacticalForeground": {
      "hsl": "0 0% 98%",
      "hex": "#FAFAFA",
      "rgb": "rgb(250, 250, 250)",
      "usage": "Text on tactical elements"
    },
    "warning": {
      "hsl": "45 93% 47%",
      "hex": "#E8A70A",
      "rgb": "rgb(232, 167, 10)",
      "usage": "Warning messages, caution states"
    },
    "warningForeground": {
      "hsl": "0 0% 10%",
      "hex": "#1A1A1A",
      "rgb": "rgb(26, 26, 26)",
      "usage": "Text on warning backgrounds"
    }
  },

  "noticeColors": {
    "border": {
      "hsl": "0 84% 60%",
      "hex": "#EB4D4D",
      "usage": "Notice/alert borders"
    },
    "background": {
      "hsl": "0 84% 60%",
      "hex": "#EB4D4D",
      "opacity": "10%",
      "usage": "Notice background with transparency"
    },
    "text": {
      "hsl": "0 0% 98%",
      "hex": "#FAFAFA",
      "usage": "Notice text"
    },
    "icon": {
      "hsl": "0 84% 60%",
      "hex": "#EB4D4D",
      "usage": "Notice icons"
    }
  },

  "spacing": {
    "unit": "0.25rem",
    "scale": {
      "0": "0",
      "1": "0.25rem",
      "2": "0.5rem",
      "3": "0.75rem",
      "4": "1rem",
      "5": "1.25rem",
      "6": "1.5rem",
      "8": "2rem",
      "10": "2.5rem",
      "12": "3rem",
      "16": "4rem",
      "20": "5rem",
      "24": "6rem"
    }
  },

  "borderRadius": {
    "none": "0",
    "sm": "calc(0.375rem - 4px)",
    "md": "calc(0.375rem - 2px)",
    "lg": "0.375rem",
    "xl": "0.75rem",
    "2xl": "1rem",
    "full": "9999px",
    "default": "0.375rem"
  },

  "shadows": {
    "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
    "default": "0 1px 3px 0 rgb(0 0 0 / 0.1), 0 1px 2px -1px rgb(0 0 0 / 0.1)",
    "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
    "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
    "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)"
  },

  "transitions": {
    "default": "all 0.2s ease-out",
    "fast": "all 0.15s ease-out",
    "slow": "all 0.3s ease-out"
  },

  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1400px"
  },

  "container": {
    "center": true,
    "padding": "2rem",
    "maxWidth": "1400px"
  },

  "componentStyles": {
    "button": {
      "primary": {
        "background": "#5C7244",
        "text": "#FAFAFA",
        "hoverBackground": "#4A5C37",
        "border": "none"
      },
      "secondary": {
        "background": "#262626",
        "text": "#F2F2F2",
        "hoverBackground": "#333333",
        "border": "none"
      },
      "outline": {
        "background": "transparent",
        "text": "#F2F2F2",
        "border": "#333333",
        "hoverBackground": "#262626"
      },
      "destructive": {
        "background": "#EB4D4D",
        "text": "#FAFAFA",
        "hoverBackground": "#D43D3D"
      },
      "ghost": {
        "background": "transparent",
        "text": "#F2F2F2",
        "hoverBackground": "#262626"
      }
    },
    "card": {
      "background": "#1A1A1A",
      "border": "#333333",
      "borderRadius": "0.375rem",
      "padding": "1.5rem"
    },
    "input": {
      "background": "#121212",
      "border": "#333333",
      "text": "#F2F2F2",
      "placeholder": "#999999",
      "focusRing": "#5C7244",
      "borderRadius": "0.375rem"
    },
    "header": {
      "background": "#121212",
      "border": "#333333",
      "text": "#F2F2F2",
      "linkHover": "#5C7244"
    },
    "footer": {
      "background": "#121212",
      "border": "#333333",
      "text": "#999999",
      "linkText": "#F2F2F2",
      "linkHover": "#5C7244"
    },
    "productCard": {
      "background": "#1A1A1A",
      "border": "#333333",
      "titleColor": "#F2F2F2",
      "priceColor": "#5C7244",
      "descriptionColor": "#999999",
      "badgeBackground": "#5C7244",
      "badgeText": "#FAFAFA"
    },
    "badge": {
      "default": {
        "background": "#262626",
        "text": "#F2F2F2"
      },
      "primary": {
        "background": "#5C7244",
        "text": "#FAFAFA"
      },
      "accent": {
        "background": "#E8A70A",
        "text": "#1A1A1A"
      },
      "destructive": {
        "background": "#EB4D4D",
        "text": "#FAFAFA"
      }
    }
  },

  "cssVariables": {
    "root": {
      "--background": "0 0% 7%",
      "--foreground": "0 0% 95%",
      "--card": "0 0% 10%",
      "--card-foreground": "0 0% 95%",
      "--popover": "0 0% 10%",
      "--popover-foreground": "0 0% 95%",
      "--primary": "84 28% 35%",
      "--primary-foreground": "0 0% 98%",
      "--secondary": "0 0% 15%",
      "--secondary-foreground": "0 0% 95%",
      "--muted": "0 0% 15%",
      "--muted-foreground": "0 0% 60%",
      "--accent": "45 93% 47%",
      "--accent-foreground": "0 0% 10%",
      "--destructive": "0 84% 60%",
      "--destructive-foreground": "0 0% 98%",
      "--border": "0 0% 20%",
      "--input": "0 0% 20%",
      "--ring": "84 28% 35%",
      "--radius": "0.375rem",
      "--tactical": "84 28% 35%",
      "--tactical-foreground": "0 0% 98%",
      "--warning": "45 93% 47%",
      "--warning-foreground": "0 0% 10%"
    }
  },

  "colorPalette": {
    "summary": {
      "darkCharcoal": "#121212",
      "charcoal": "#1A1A1A",
      "darkGray": "#262626",
      "mediumGray": "#333333",
      "gray": "#999999",
      "lightGray": "#F2F2F2",
      "white": "#FAFAFA",
      "oliveGreen": "#5C7244",
      "goldenAmber": "#E8A70A",
      "alertRed": "#EB4D4D"
    }
  }
}
